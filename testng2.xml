<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Parallel Execution" parallel="tests" thread-count="1">
    <test name="Sample Test">
        <parameter name="browser" value="chrome" />

        <classes>
            <class name="sample">
                <methods>
                    <!-- Include specific test methods -->
                    <include name="test1"/>
                    <!-- Exclude specific test methods -->
                    <exclude name="test2"/>
                </methods>
            </class>
            
        </classes>
    </test>

    <test name="fire">
        <parameter name="browser" value="firefox" />
        <classes>
            <class name="twoBrowser" />
            <methods>
                <!-- Include specific test methods -->
                <include name="test1"/>
                <!-- Exclude specific test methods -->
                <exclude name="test2"/>
            </methods>
        </classes>
    </test>

</suite>