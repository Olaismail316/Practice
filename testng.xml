<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="Parallel Execution" parallel="tests" thread-count="1">
    <test name="firefox">
        <parameter name="browser" value="firefox" />
        <classes>java
            <class name="twoBrowser">
                <methods>
                    <include name="test2"/>

                    <include name="test1"/>

                </methods>
            </class>
        </classes>
    </test>
    <test name="chrome">
        <parameter name="browser" value="chrome" />
        <classes>
            <class name="sample">
                <methods>
                    <include name="testYahooSearch"/>
                    <include name="testGoogleSearch"/>
                </methods>
            </class>
        </classes>
    </test>


</suite>
